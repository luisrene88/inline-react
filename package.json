  "devDependencies": {
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "concurrently": "^3.5.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-slick": "^0.14.11"
  }
